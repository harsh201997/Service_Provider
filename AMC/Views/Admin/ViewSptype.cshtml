@{
    ViewBag.Title = "View Service Provider Type";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="table-agile-info">
    <div class="panel panel-default">
        <div class="panel-heading">
            Services
        </div>
        <div>
            <script>
    $(document).ready(function () {
        $('#tb').DataTable();
    });
            </script>

            <link href="~/jquery.dataTables.min.css" rel="stylesheet" />
            <script src="~/jquery.dataTables.min.js"></script>
            <table id="tb" class="table" ui-jq="footable" ui-options='{
        "paging": {
          "enabled": true
        },
        "filtering": {
          "enabled": true
        },
        "sorting": {
          "enabled": true
        }}'>
                <thead>
                    <tr>

                        <th>Service</th>
                        <th data-breakpoints="xs sm md">Delete</th>
                        <th data-breakpoints="xs sm md">Update</th>
                    </tr>
                </thead>
                <tbody>

                    @foreach (var item in ViewData["temp"] as List<AMC.EDM.ServiceproviderType>)
                    {

                        <tr data-expanded="true">

                            <td>@item.sptype_name</td>
                            <td><a href="/Admin/DeleteSpType/@item.sptype_id" onclick="return confirm('Are you sure? You want to Delete this record?');" class="btn btn-danger">Delete</a></td>
                            <td><a href="/Admin/UpdateSpType/@item.sptype_id" onclick="return confirm('Are you sure? You want to Edit this record?');" class="btn btn-warning">Update</a></td>

                        </tr>

                    }
                   
                </tbody>
            </table>
        </div>
    </div>
</div>