@{
    ViewBag.Title = "ViewSp";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="table-agile-info">
    <div class="panel panel-default">
        <div class="panel-heading">
           City List
        </div>
        <div>
            <script>  $(document).ready(function ()
                         {
                         $('#tb').DataTable();
                         });
            </script>

            <link href="~/jquery.dataTables.min.css" rel="stylesheet" />
            <script src="~/jquery.dataTables.min.js"></script>
            <table id="tb" class="table" ui-jq="footable" ui-options='{
        "paging": {
          "enabled": true
        },
        "filtering": {
          "enabled": true
        },
        "sorting": {
          "enabled": true
        }}'>
                <thead>
                    <tr>
                       
                        <th>City Name</th>
                      
                        <th data-breakpoints="xs">update</th>
                    </tr>
                </thead>
                <tbody>

                    @foreach (var item in ViewData["temp"] as List<AMC.EDM.City_master>)
                    {

                        <tr data-expanded="true">
                           
                            <td>@item.city_name</td>                           
                           <td><a href="/Admin/DeleteCity/@item.city_id" onclick="return confirm('Are you sure? You want to Delete this record?');" class="btn btn-danger">Delete</a></td>
                      
                        </tr>
                        

   
    }

                </tbody>
            </table>
        </div>
    </div>
</div>


