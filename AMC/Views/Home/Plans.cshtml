@{
    ViewBag.Title = "Plans";
    Layout = "~/Views/Shared/HomeLayout.cshtml";
}

<!-- breadcrumbs -->
<div class="breadcrumbs-w3l">
    <div class="container">
        <span class="breadcrumbs">
            <a href="index.html">Home</a> |
            <span>Plans</span>
        </span>
    </div>
</div>

<!-- plan -->
<div class="w3ls-section plans">
    <div class="container">
        <h4 class="main-title">plans</h4>
        <div class="services-main">
            <!-- maintenance essential plan -->
           

    
  @foreach (var item in ViewData["t1"] as List<AMC.EDM.Tbl_plan>)
  {


    <div class="col-md-4 col-sm-4 w3_agile-plan-grid">
        <div class="pricing_grid_content">
            <h5>@item.plan_name</h5>
            <h3>
                <sup>Rs.</sup>
                <span class="counter agile_counter">
                    @item.charges /

                    <label>@item.plan_duration</label>
                </span>
            </h3>
            <ul class="w3_count">
                <li>
                    <span class="fa fa-check" aria-hidden="true"></span>
                    <p>@item.plan_benifits</p>
                </li>
                <li>
                    <span class="fa fa-check" aria-hidden="true"></span>
                    <p> @item.plan_description</p>
                </li>
                
            </ul>
            <a href="~/Home/UserRegistration">Enrolled Plan</a>
        </div>
    </div>
}
            <div class="clearfix"></div>

         
        </div>
    </div>
</div>
<!-- //plan -->
