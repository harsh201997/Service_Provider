@{
    ViewBag.Title = "Inprocessinquiry";
    Layout = "~/Views/Shared/customerlayout.cshtml";
}


<div class="table-agile-info">
    <div class="panel panel-default">
        <div class="panel-heading">
            Service Inquiry Detail
        </div>
        <div>
            <script>
  $(document).ready(function () {
      $('#tb').DataTable();
  });
            </script>

            <link href="~/jquery.dataTables.min.css" rel="stylesheet" />
            <script src="~/jquery.dataTables.min.js"></script>
            <table id="tb" class="table" ui-jq="footable" ui-options='{
        "paging": {
          "enabled": true
        },
        "filtering": {
          "enabled": true
        },
        "sorting": {
          "enabled": true
        }}'>
                <thead>
                    <tr>
                        <th data-breakpoints="xs">Service Provider</th>
                        <th data-breakpoints="xs">Service</th>
                        <th data-breakpoints="xs">Mobile-No</th>
                        <th data-breakpoints="xs">Complaint Date</th>
                        <th data-breakpoints="xs">Process Date</th>
                        <th data-breakpoints="xs">Complaint Status</th>

                        <th data-breakpoints="xs">Description</th>




                    </tr>
                </thead>
                <tbody>

                    @foreach (var item in ViewData["temp"] as List<AMC.Models.ServiceInquiry>)
                    {

                        <tr data-expanded="true">
                            <td>@item.sp_fname @item.sp_lname</td>
                            <td>@item.sptype_name</td>
                            <td>@item.sp_mobile</td>
                            <td>@item.date</td>
                            <td>@item.processdate</td>
                            <td>@item.status</td>
                            <td>@item.description</td>



                        </tr>

                    }
                   
                </tbody>
            </table>
        </div>
    </div>
</div>