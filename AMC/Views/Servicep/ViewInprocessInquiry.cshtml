


@{
    ViewBag.Title = "ViewSp";
    Layout = "~/Views/Shared/sproviderlayout.cshtml";
}

<div class="table-agile-info">
    <div class="panel panel-default">
        <div class="panel-heading">
            Service Inquiry Detail
        </div>
        <div>
            <script>
  $(document).ready(function () {
      $('#tb').DataTable();
  });
            </script>

            <link href="~/jquery.dataTables.min.css" rel="stylesheet" />
            <script src="~/jquery.dataTables.min.js"></script>
            <table id="tb" class="table" ui-jq="footable" ui-options='{
        "paging": {
          "enabled": true
        },
        "filtering": {
          "enabled": true
        },
        "sorting": {
          "enabled": true
        }}'>
                <thead>
                    <tr>

                        <th data-breakpoints="xs">Complaint Date</th>
                        <th data-breakpoints="xs">Process Date</th>
                        <th data-breakpoints="xs">Complaint Status</th>

                        <th data-breakpoints="xs">Description</th>
                        <th data-breakpoints="xs">Update Status</th>

                       
                        <th data-breakpoints="xs">Customer Detail</th>

                    </tr>
                </thead>
                <tbody>

                    @foreach (var item in ViewData["temp"] as List<AMC.Models.ServiceInquiry>)
                    {

                        <tr data-expanded="true">
                            <td>@item.date</td>
                            <td>@item.processdate</td>
                            <td>@item.status</td>
                            <td>@item.description</td>



                            <td><a href="/Servicep/UpdateInquiryStatus/@item.userreq_id">Done</a></td>
                           
                            <td><a href="/Servicep/UserDetail/@item.user_id">Customer Detail</a></td>

                        </tr>

                    }
                   
                </tbody>
            </table>
        </div>
    </div>
</div>