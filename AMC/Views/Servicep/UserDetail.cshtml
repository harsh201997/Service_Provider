@{
    ViewBag.Title = "ViewSp";
    Layout = "~/Views/Shared/sproviderlayout.cshtml";
}


<div class="table-agile-info">
    <div class="panel panel-default">
        <div class="panel-heading">
            Customer Detail
        </div>
        <div>
            <table class="table" ui-jq="footable" ui-options='{
        "paging": {
          "enabled": true
        },
        "filtering": {
          "enabled": true
        },
        "sorting": {
          "enabled": true
        }}'>
                <thead>
                    <tr>
                        <th>Customer Name</th>
                        <th>Mobile No</th>
                        <th>Email Id</th>
                        <th data-breakpoints="xs">Address</th>                    
                        <th data-breakpoints="xs">Image</th>                       
                    </tr>
                </thead>
                <tbody>
                   @{var p = ViewData["temp"] as AMC.EDM.User_regis;
                  

                        <tr data-expanded="true">
                            <td>@p.fname @p.lname</td>
                            <td>@p.mobile</td>
                            <td>@p.email</td>
                            <td>@p.address</td>                            
                         
                            <td><img src="@p.user_img" height="100" width="100" /></td>
                           

                        </tr>

                    }
                   
                </tbody>
            </table>
        </div>
    </div>
</div>