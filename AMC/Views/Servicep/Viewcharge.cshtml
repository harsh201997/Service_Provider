@{
    ViewBag.Title = "Viewcharge";
    Layout = "~/Views/Shared/sproviderlayout.cshtml";
}


<div class="table-agile-info">
    <div class="panel panel-default">
        <div class="panel-heading">
            Service Charge
        </div>
        <div>
            <script>
    $(document).ready(function () {
        $('#tb').DataTable();
    });
            </script>

            <link href="~/jquery.dataTables.min.css" rel="stylesheet" />
            <script src="~/jquery.dataTables.min.js"></script>
            <table id="tb" class="table" ui-jq="footable" ui-options='{
        "paging": {
          "enabled": true
        },
        "filtering": {
          "enabled": true
        },
        "sorting": {
          "enabled": true
        }}'>
                <thead>
                    <tr>
                        
                        <th>Charges</th>
                       
                       
                        <th data-breakpoints="xs">Update</th>

                        
                    </tr>
                </thead>
                <tbody>

                    @foreach (var item in ViewData["temp"] as List<AMC.EDM.Charge_rate>)
                    {

                        <tr data-expanded="true">
                            <td>@item.amount</td>
                           
                            <td><a href="/Servicep/UpdateCharge/@item.charge_id" onclick="return confirm('Are you sure? You want to Edit this record?');" class="btn btn-warning">Edit</a></td>


                        </tr>

                    }
                  
                </tbody>
            </table>
        </div>
    </div>
</div>